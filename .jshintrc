{
  "boss": true,
  "browser": true,
  "camelcase": false,
  "eqeqeq": true,
  "eqnull": true,
  "es5": false,
  "esnext": true,
  "expr": true,
  "forin": false,
  "globals": {
    "define": false
  },
  "indent": 2,
  "laxbreak": true,
  "laxcomma": true,
  "maxerr": 100,
  "maxlen": 80,
  "node": true,
  "noarg": true,
  "passfail": false,
  "predef": [
